{
  "name": "react-tag-input",
  "version": "0.0.0-semantically-released",
  "description": "React tags is a fantastically simple tagging component for your React projects",
  "main": "dist-modules/components/ReactTags.js",
  "files": [
    "dist-modules",
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "webpack --config webpack-production.config.js --progress --colors && babel src --out-dir dist-modules --copy-files",
    "commit": "git-cz",
    "format": "prettier  --write src/*.js __tests__/*.js",
    "lint": "esw webpack* src --color",
    "lint:fix": "esw webpack* src --color --fix",
    "npm-check:update": "npm-check -u",
    "precommit": "./set_up_hooks.sh",
    "semantic-release": "semantic-release",
    "start": "webpack-dev-server --config webpack-dev-server.config.js --progress",
    "test": "jest --notify --coverage",
    "test:watch": "jest --watch --notify --coverage"
  },
  "keywords": [
    "react",
    "drag-drop",
    "tags",
    "tag input",
    "react-component",
    "autosuggest"
  ],
  "author": "Prakhar Srivastav",
  "license": "MIT",
  "repository": "https://github.com/facetdigital/react-tags",
  "peerDependencies": {
    "react": "16.13.1",
    "react-dnd": "10.0.2",
    "react-dnd-html5-backend": "10.0.2",
    "react-dom": "16.13.1"
  },
  "dependencies": {
    "classnames": "2.2.6",
    "core-js": "3.6.4",
    "lodash": "4.17.15",
    "memoize-one": "5.1.1",
    "prop-types": "15.7.2"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.8.7",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "7.8.3",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-destructuring": "7.8.8",
    "@babel/plugin-transform-react-constant-elements": "7.8.3",
    "@babel/plugin-transform-react-jsx": "7.8.3",
    "@babel/preset-env": "7.8.7",
    "@babel/preset-react": "7.8.3",
    "@babel/register": "7.8.6",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "25.1.0",
    "babel-loader": "8.0.6",
    "chai": "4.2.0",
    "commitizen": "4.0.3",
    "css-loader": "3.4.2",
    "cz-conventional-changelog": "3.1.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "eslint": "6.8.0",
    "eslint-plugin-jest": "23.8.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.19.0",
    "eslint-watch": "6.0.1",
    "greenkeeper-lockfile": "1.15.1",
    "jest": "25.1.0",
    "jsdom": "16.2.1",
    "node-sass": "^4.13.1",
    "npm-run-all": "4.1.5",
    "opn": "6.0.0",
    "prettier": "1.19.1",
    "raf": "3.4.1",
    "react": "16.13.1",
    "react-dnd": "10.0.2",
    "react-dnd-html5-backend": "10.0.2",
    "react-dnd-test-backend": "10.0.2",
    "react-dnd-test-utils": "10.0.2",
    "react-dom": "16.13.1",
    "react-test-renderer": "16.13.1",
    "sass-loader": "8.0.2",
    "semantic-release": "17.0.4",
    "sinon": "9.0.1",
    "style-loader": "1.1.3",
    "webpack": "4.42.0",
    "webpack-bundle-analyzer": "3.6.1",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.10.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
