{
  "env": {
    "development": {
      "plugins": []
    },
    "production": {
      "plugins": []
    },
    "test": {
      "plugins": [
        "@babel/plugin-proposal-class-properties"
      ]
    }
  },
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "usage",
        "corejs": 3,
        "debug": false,
        "modules": "commonjs"
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-syntax-dynamic-import",
    "@babel/plugin-proposal-object-rest-spread",
    "@babel/plugin-transform-destructuring",
    "@babel/plugin-transform-react-jsx"
  ]
}
